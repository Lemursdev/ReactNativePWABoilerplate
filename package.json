{
  "name": "ReactNativePWABoilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "dev": "npm run web",
    "watch": "npm run web",
    "web": "webpack serve --content-base builds/web/ --config config/web/webpack.dev.config.js --mode development --port 3001 --inline --hot",
    "build": "npm run web-bundle",
    "web-bundle": "webpack --config config/web/webpack.prod.config.js --progress --colors --mode production",
    "test": "mocha",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "i18next": "^19.8.3",
    "react": "16.13.1",
    "react-dom": "^17.0.1",
    "react-i18next": "^11.7.3",
    "react-native": "0.63.3",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-router-native": "^5.2.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@reduxjs/toolkit": "^1.4.0",
    "@types/jest": "^25.2.3",
    "@types/react-native": "^0.63.2",
    "@types/react-redux": "^7.1.11",
    "@types/react-test-renderer": "^16.9.2",
    "@types/redux-logger": "^3.0.8",
    "@types/redux-thunk": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.1.0",
    "babel-polyfill": "^6.26.0",
    "eslint": "^6.5.1",
    "eslint-plugin-mocha": "^8.0.0",
    "html-webpack-plugin": "^4.5.0",
    "json5-loader": "^4.0.1",
    "metro-react-native-babel-preset": "^0.59.0",
    "mocha": "^8.2.1",
    "react-test-renderer": "16.13.1",
    "redux-logger": "^3.0.6",
    "sass-loader": "^10.0.4",
    "standard": "^16.0.1",
    "ts-loader": "^8.0.7",
    "typescript": "^4.0.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.3.1",
    "webpack-cli": "^4.1.0",
    "webpack-dev-server": "^3.11.0"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
